 <!-- page head start-->
<div class="page-head">
  <h3 class="m-b-less">
  Admin Users 
  </h3>
</div>
<!-- page head end-->

<!--body wrapper start-->
<div class = "wrapper">
  <div class = "panel">
    <div class ="row"> 
      <div class="col-sm-12">
        <header class="panel-heading head-border">
          <a class="btn btn-success" href="<%#= new_credit_card_path %>">Add New</a>
          <span class="tools pull-right">
              <a class="fa fa-repeat box-refresh" href="javascript:;"></a>
              <a class="t-close fa fa-times" href="javascript:;"></a>
          </span>
        </header>
        <div class="panel-body">
          <div>
            <table class="table colvis-data-table data-table">
              <thead>
                <tr>
                  <th>Name</th>
                  <th>Email</th>
                  <th>Role</th>
                  <th>Created at</th>
                </tr>
              </thead>
              <tbody>
                <% @admin_users.each do |admin_user| %>
	                <tr>
	                  <td><%= admin_user.full_name%><%#= link_to "SHOW",credit_card_path(credit_card)%></td>
	                  <td><%= admin_user.email%></td>
	                  <td><%= admin_user.role %></td>
	                  <td><%= admin_user.created_at.strftime("%d %B %Y") %></td>
	                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>  
<!--body wrapper send